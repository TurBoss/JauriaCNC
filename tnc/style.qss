QPushButton{
    width:60px;
    height:48px;
    border-style: solid;
    border-color: #000000;
    border-width: 1px;
    border-radius: 5px;
    color: #7f4000;
    padding: 2px;
    background-color: #424242;
}
QPushButton::default{
    border-style: solid;
    border-color: #050a0e;
    border-width: 1px;
    border-radius: 5px;
    color: #7f4000;
    padding: 2px;
}


QPushButton:enabled{
    border-style: solid;
    border-top-color: qlineargradient(spread:pad, x1:0, y1:1, x2:1, y2:1, stop:0 #7f4000, stop:0.4 #7f4000, stop:0.5 #424242, stop:1 #424242);
    border-bottom-color: qlineargradient(spread:pad, x1:0, y1:1, x2:1, y2:1, stop:0 #424242, stop:0.5 #424242, stop:0.6 #7f4000, stop:1 #7f4000);
    border-left-color: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop:0 #ff7f00, stop:0.3 #7f4000, stop:0.7 #424242, stop:1 #424242);
    border-right-color: qlineargradient(spread:pad, x1:0, y1:1, x2:0, y2:0, stop:0 #7f4000, stop:0.3 #7f4000, stop:0.7 #424242, stop:1 #424242);
    border-width: 2px;
    border-radius: 1px;
    padding: 2px;
}


QPushButton:disabled{
    border-style: solid;
    border-top-color: qlineargradient(spread:pad, x1:0, y1:1, x2:1, y2:1, stop:0 #202020, stop:0.4 #202020, stop:0.5 #424242, stop:1 #424242);
    border-bottom-color: qlineargradient(spread:pad, x1:0, y1:1, x2:1, y2:1, stop:0 #424242, stop:0.5 #424242, stop:0.6 #202020, stop:1 #202020);
    border-left-color: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop:0 #202020, stop:0.3 #202020, stop:0.7 #424242, stop:1 #424242);
    border-right-color: qlineargradient(spread:pad, x1:0, y1:1, x2:0, y2:0, stop:0 #202020, stop:0.3 #202020, stop:0.7 #424242, stop:1 #424242);
    border-width: 2px;
    border-radius: 1px;
    padding: 2px;
}

QPushButton:pressed{
    border-style: solid;
    border-top-color: qlineargradient(spread:pad, x1:0, y1:1, x2:1, y2:1, stop:0 #ff7f00, stop:0.4 #ff7f00, stop:0.5 #424242, stop:1 #424242);
    border-bottom-color: qlineargradient(spread:pad, x1:0, y1:1, x2:1, y2:1, stop:0 #424242, stop:0.5 #424242, stop:0.6 #ff7f00, stop:1 #ff7f00);
