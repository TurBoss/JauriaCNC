    border-right-color: qlineargradient(spread:pad, x1:0, y1:1, x2:0, y2:0, stop:0 #ff7f00, stop:0.3 #ff7f00, stop:0.7 #424242, stop:1 #424242);
    border-width: 2px;
    border-radius: 1px;
    color: #000000;
    background-color: #7f4000;
    padding: 2px;
}

QPushButton:checked{
    border-style: solid;
    border-top-color: qlineargradient(spread:pad, x1:0, y1:1, x2:1, y2:1, stop:0 #ff7f00, stop:0.4 #ff7f00, stop:0.5 #424242, stop:1 #424242);
